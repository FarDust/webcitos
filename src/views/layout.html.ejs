<!DOCTYPE html>
<html>

  <head>
    <meta charset="utf-8">
    <title>IIC2513 - WebCitos</title>
    <% if (env !== 'development') { %>
      <link rel="stylesheet" href="<%= assetPath('app.css') %>">
    <% } %>
    <link rel="stylesheet" href="<%= assetPath('fardust.css') %>">
  </head>

  <body>
    <!--logo-->
    <div class="container top" id="home">
      <a href="/" class="logo">
        Web<span class="Music">Citos</span>
      </a>
    </div>
    <!--topbar-->
    <ul class="topbar">
      <li>
        <span id="side-open" onclick="openNav()">
          <img class="side-menu center" src="<%= assetPath('side.png') %>" >
        </span>
      </li>
      <li><a href="/">Home</a></li>
    </ul>
    <!--sidebar-->
    <div id="right-arrow" class="circle" onclick="openNav()" style="top:50%">
      <span class="center arrow">
        <i class="arrowr"></i>
      </span>
    </div>
    
    <div class="sidenav" id="lateral-menu">
      <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">
        &times;
      </a>
      <a class="side" href="../index.html">Home</a>
    </div>
    <%- body %>
    <script src="<%= assetPath('app.js') %>"></script>
  </body>

</html>
