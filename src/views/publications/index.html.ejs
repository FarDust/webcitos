<div id="react-main" data-publications="<%= JSON.stringify(publications) %>" data-userNames="<%=JSON.stringify(users_names)%>" data-itemsIds="<%=JSON.stringify(items_ids)%>">
    <div class="pcard">
        <% publications.forEach((publication) => { %>
          <a class="box" href="<%= getShowPath(publication) %>">
            <h4><%= publication.title %></h4>
            <img class="imgcard" src="<%= getItemImagePath(items_ids[publication.id]) %>" alt="Photo of the item"/>
            <p><%= publication.description %></p><br>
            <span>State:</span> <%= publication.state %> <br>
            <span>Added by:</span> <%= users_names[publication.id] %> <br>
          </a>
          <% }) %>
    </div>
</div>
