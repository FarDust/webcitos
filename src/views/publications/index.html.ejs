<div class="products">
    <div class="pcard">
        <% publications.forEach((publication) => { %>
          <a class="box" href="<%= getShowPath(publication) %>" style="text-decoration:none;">
            
            <h4><%= publication.title %></h4>
            <%= publication.description %><br><br>
            <span>State:</span> <%= publication.state %> <br>
            <span>Added by:</span> <%= publication.userID %> <br>
          </a>
          <% }) %>
    </div>
</div>